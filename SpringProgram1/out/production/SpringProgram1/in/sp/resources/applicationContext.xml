<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean class="in.sp.beans.Student" id="stdId">
        <property name = "name" value = "Durvesh" />
        <property name = "rollNo" value = "10" />
        <property name = "email" value = "Durvesh@gmail.com" />
    </bean>

    <bean class="in.sp.beans.Employees" id="EmployeeId">
        <property name = "EmpID" value = "101" />
        <property name = "name" value = "Durvesh Vijay Wagh" />
        <property name = "CompanyName" value = "MasterCard" />
    </bean>

</beans>